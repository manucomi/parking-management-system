{
    "name": "parking-management-system",
    "private": true,
    "version": "1.0.0",
    "packageManager": "npm@8.5.0",
    "scripts": {
        "build": "dotenv turbo run build -- --concurrency=50%",
        "change:add": "changeset add && git add .changeset/*.md",
        "change:add:empty": "changeset add --empty && git add .changeset/*.md",
        "change:version": "changeset version && git add . && git commit -m 'chore: version bump' || echo 'No version bump needed'",
        "clean": "rm -rf .tmp/ .next/ && npm run clean -ws --if-present",
        "clean:all": "npm run clean && rm -rf node_modules/ && npm run clean:all -ws --if-present",
        "dev": "dotenv turbo run dev",
        "lint": "npm run lint -ws --if-present",
        "lint:fix": "npm run lint:fix -ws --if-present",
        "lint:js": "eslint --ext .js,.jsx .",
        "lint:js:fix": "eslint --fix --ext .js,.jsx .",
        "prepare": "husky || true",
        "release:notes": "release-it --ci",
        "storybook": "npm run -w frontend storybook",
        "test": "dotenv turbo run test --concurrency=50%"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "6.8.0",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
        "dotenv-cli": "^10.0.0",
        "eslint": "8.57.1",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-next": "15.5.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-import-resolver-alias": "1.1.2",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jest-dom": "5.5.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-storybook": "9.1.5",
        "eslint-plugin-testing-library": "7.6.8",
        "husky": "9.1.7",
        "jest": "30.1.3",
        "jest-environment-jsdom": "30.1.2",
        "postcss": "^8.5.6",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^10.3.1",
        "postcss-pxtorem": "^6.1.0",
        "postcss-scss": "4.0.9",
        "prettier": "3.6.2",
        "sass": "1.92.1",
        "storybook": "9.1.5",
        "stylelint": "16.24.0",
        "stylelint-config-sass-guidelines": "12.1.0",
        "stylelint-order": "6.0.4",
        "stylelint-prettier": "5.0.3",
        "turbo": "^2.5.8"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "dependencies": {
        "lucide-react": "^0.545.0"
    }
}
