{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "change:add": "changeset add && git add .changeset/*.md",
        "change:add:empty": "changeset add --empty && git add .changeset/*.md",
        "clean": "npm run clean:build && npm run clean:deps && npm run clean:test",
        "clean:build": "rm -rf .next/",
        "clean:deps": "rm -rf node_modules/",
        "clean:test": "rm -rf .coverage/",
        "dev": "next dev",
        "lint": "npm run lint:js && npm run lint:scss",
        "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
        "lint:js": "eslint --ext .js,.jsx .",
        "lint:js:fix": "eslint --fix --ext .js,.jsx .",
        "lint:scss": "stylelint -f verbose '**/*.{css,scss}'",
        "lint:scss:fix": "npm run lint:scss -- --fix",
        "prepare": "husky",
        "prettier": "npx prettier --write .",
        "start": "next start",
        "test": "jest",
        "storybook": "storybook dev -p 6006"
    },
    "dependencies": {
        "next": "15.5.2",
        "prop-types": "15.8.1",
        "react": "19.1.0",
        "react-dom": "19.1.0"
    },
    "devDependencies": {
        "@changesets/cli": "2.29.4",
        "@storybook/addon-a11y": "9.1.5",
        "@storybook/addon-docs": "9.1.5",
        "@storybook/addon-onboarding": "9.1.5",
        "@storybook/addon-vitest": "9.1.5",
        "@storybook/nextjs-vite": "9.1.5",
        "@testing-library/jest-dom": "6.8.0",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
        "@vitest/browser": "3.2.4",
        "@vitest/coverage-v8": "3.2.4",
        "axe-playwright": "2.2.2",
        "eslint": "8.57.1",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-next": "15.5.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-import-resolver-alias": "1.1.2",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jest-dom": "5.5.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-storybook": "9.1.5",
        "eslint-plugin-testing-library": "7.6.8",
        "husky": "9.1.7",
        "jest": "30.1.3",
        "jest-environment-jsdom": "30.1.2",
        "msw": "2.11.3",
        "msw-storybook-addon": "2.0.5",
        "playwright": "^1.55.0",
        "postcss": "^8.5.6",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^10.3.1",
        "postcss-pxtorem": "^6.1.0",
        "postcss-scss": "4.0.9",
        "prettier": "3.6.2",
        "sass": "1.92.1",
        "storybook": "9.1.5",
        "stylelint": "16.24.0",
        "stylelint-config-sass-guidelines": "12.1.0",
        "stylelint-order": "6.0.4",
        "stylelint-prettier": "5.0.3",
        "vitest": "3.2.4"
    }
}
